    "ioredis": "5.4.1",
    "joi": "17.13.1",
    "nodemailer": "6.9.13",
    "pdf-lib": "1.17.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.8",
    "@types/bcrypt": "5.0.2",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.4",
    "@types/node": "20.12.12",
