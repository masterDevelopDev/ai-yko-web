    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "exceljs": "4.4.0",
    "fast-xml-parser": "4.4.0",
    "jest": "29.7.0",
    "knip": "5.16.0",
    "lodash": "4.17.21",
    "prettier": "3.2.5",
    "prisma": "5.14.0",
    "slugify": "1.6.6",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.1.2",
